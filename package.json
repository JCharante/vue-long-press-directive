{
  "name": "vue-long-press-directive",
  "version": "1.0.0",
  "description": "Long press directive plugin for Vue.js",
  "main": "index.js",
  "scripts": {
    "demo-watch": "wzrd demo/index.js:demo/bundle.js -- -t vueify",
    "demo-build": "browserify demo/index.js -o demo/bundle.js -t vueify",
    "test-build": "mkdir -p dist/test && browserify test/index.js -o dist/test/index.js -t vueify --node --no-bundle-external",
    "test": "npm run lint && npm run test-build && tape dist/test",
    "lint": "standard --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kapetan/vue-long-press-directive.git"
  },
  "keywords": [
    "vue",
    "plugin",
    "directive",
    "long",
    "press",
    "touch",
    "mouse"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kapetan/vue-long-press-directive/issues"
  },
  "homepage": "https://github.com/kapetan/vue-long-press-directive#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.4.0",
    "jsdom": "^11.3.0",
    "standard": "^10.0.3",
    "tape": "^4.8.0",
    "vue": "^2.4.4",
    "vueify": "^9.4.1",
    "wzrd": "^1.5.0"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  }
}
